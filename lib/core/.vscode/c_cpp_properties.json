{
    "configurations": [
        {
            "name": "debug",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "cStandard": "gnu17",
            "intelliSenseMode": "gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/**"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "compilerPath": "/usr/bin/gcc",
            "defines": [
                "DEBUG=1"
            ]
        },
        {
            "name": "release",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "DEBUG=0"
            ],
            "cStandard": "gnu17",
            "intelliSenseMode": "gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/**"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "compilerPath": "/usr/bin/gcc"
        }
    ],
    "version": 4
}